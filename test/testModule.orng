testModule::Module = (
	A::Type = <a, b, c:Int>
	B::Type = <c:Int, d:Char, e>

	Complex::Type = (
		re:Real
		im:Real

		add::(c1:Complex, c2:Complex)->Complex = (.re=c1.re + c2.re, .im=c1.im + c2.im)
		sub::(c1:Complex, c2:Complex)->Complex = (.re=c1.re - c2.re, .im=c1.im - c2.im)
	)

	FileError::Type = <
		fileNotFound:Int
		resourceBusy
	>

	AllocationError::Type = <
		outOfMemory
		resourceBusy
	>

	lol::(x:Int)->!Int = {
		if x == 0 {
			return FileError.fileNotFound
		} else {
			return x
		}
	}

	recursive::(x:Int)->() = {return recursive(x-1)}

	main::(args:[]String)->!() = std.debug.assert(.msg="this should be true", .predicate=false)
)